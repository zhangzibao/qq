<template name="029fc030">
  <view class="_view data-v-2684d710 page">
    <view class="_view data-v-2684d710 img-view" wx:if="{{hasLogin}}">
      <image src="{{userData.uAvatar}}" class="_image data-v-2684d710"></image>
    </view>
    <view class="_view data-v-2684d710 uni-list-cell" hover-class="uni-list-cell-hover" style="background-color: #fff">
      <view class="_view data-v-2684d710 uni-media-list">
        <image class="_image data-v-2684d710 uni-media-list-logo" src="../../static/icon/phone.png"></image>
        <view class="_view data-v-2684d710 uni-media-list-body">
          <view class="_view data-v-2684d710 uni-media-list-text-top">手机</view>
          <view class="_view data-v-2684d710 uni-media-list-text-bottom">
            <text class="_text data-v-2684d710">{{userData.uPhone}}</text>
          </view>
        </view>
      </view>
    </view>
    <view class="_view data-v-2684d710 uni-list-cell" hover-class="uni-list-cell-hover" style="background-color: #fff">
      <view class="_view data-v-2684d710 uni-media-list">
        <image class="_image data-v-2684d710 uni-media-list-logo" src="../../static/icon/address.png"></image>
        <view class="_view data-v-2684d710 uni-media-list-body">
          <view class="_view data-v-2684d710 uni-media-list-text-top">位置</view>
          <view class="_view data-v-2684d710 uni-media-list-text-bottom">
            <text class="_text data-v-2684d710">{{userData.uAddress}}</text>
          </view>
        </view>
      </view>
    </view>
    <view class="_view data-v-2684d710 uni-list-cell" hover-class="uni-list-cell-hover" style="background-color: #fff">
      <view class="_view data-v-2684d710 uni-media-list">
        <image class="_image data-v-2684d710 uni-media-list-logo" src="../../static/icon/birthday.png"></image>
        <view class="_view data-v-2684d710 uni-media-list-body">
          <view class="_view data-v-2684d710 uni-media-list-text-top">生日</view>
          <view class="_view data-v-2684d710 uni-media-list-text-bottom">
            <text class="_text data-v-2684d710">{{userData.uBirthday}}</text>
          </view>
        </view>
      </view>
    </view>
    <view class="_view data-v-2684d710 myfooter" wx:if="{{activeId === self.id}}">
      <button type="primary" disabled="true" plain="true" style="width: 80%" class="_button data-v-2684d710">这是我自己</button>
    </view>
    <view class="_view data-v-2684d710 myfooter" wx:elif="{{friend !== undefined}}">
      <button type="primary" disabled="true" plain="true" class="_button data-v-2684d710">打电话</button>
      <button type="primary" bindtap="handleProxy" data-eventid="{{'A4f-0'}}" data-comkey="{{$k}}" class="_button data-v-2684d710">发消息</button>
    </view>
    <view class="_view data-v-2684d710 myfooter" wx:else>
      <button type="primary" bindtap="handleProxy" style="width: 80%" data-eventid="{{'vsN-1'}}" data-comkey="{{$k}}" class="_button data-v-2684d710">加好友</button>
    </view>
  </view>
</template>