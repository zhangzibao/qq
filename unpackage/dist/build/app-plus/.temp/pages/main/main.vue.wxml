<template name="1dce3a70">
  <view class="_view data-v-24c259f8">
    <view class="_view data-v-24c259f8 noMessage" wx:if="{{commitInfo.length===0}}">
      <view style="color: #C3C3C3;" class="_p data-v-24c259f8">暂时没有新消息</view>
    </view>
    <view wx:if="{{hasLogin&&commitInfo!==undefined}}" class="_view data-v-24c259f8 content">
      <view class="_div data-v-24c259f8 container_of_slide" wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'gpG-6-'+index}}" data-comkey="{{$k}}" wx:for="{{commitInfo}}" wx:for-index="index" wx:for-item="item">
        <view class="_div data-v-24c259f8 slide_list" bindtouchstart="handleProxy" bindtouchend="handleProxy" bindtouchmove="handleProxy" data-eventid="{{'jv8-5-'+index}}" data-comkey="{{$k}}" style=" {{('transform:' + ('translate3d(' + item.slide_x + 'px, 0, 0)') + ';')}}">
          <view class="_div data-v-24c259f8 now-message-info" style=" {{('width:' + (Screen_width + 'px') + ';')}}">
            <view class="_div data-v-24c259f8 imgInfo" bindtap="handleProxy" data-eventid="{{'bZz-0-'+index}}" data-comkey="{{$k}}">
              <image src="{{item.uAvatar}}" class="_image data-v-24c259f8"></image>
            </view>
            <view class="_div data-v-24c259f8 centerInfo">
              <view class="_p data-v-24c259f8 name"> {{item.nickname}} </view>
              <view class="_p data-v-24c259f8 message"> {{item.list.messageContent}} </view>
            </view>
            <view class="_div data-v-24c259f8 timeInfo" bindtap="handleProxy" data-eventid="{{'QqP-1-'+index}}" data-comkey="{{$k}}">
              <view class="_div data-v-24c259f8 time">
                <text class="_text data-v-24c259f8"> {{item.list.addTime}} </text>
              </view>
            </view>
          </view>
          <view class="_div data-v-24c259f8 group-btn">
            <view class="_div data-v-24c259f8 top" bindtap="handleProxy" wx:if="{{item.top}}" data-eventid="{{'AIZ-3-'+index}}" data-comkey="{{$k}}"> 取消置顶 </view>
            <view class="_div data-v-24c259f8 top" bindtap="handleProxy" wx:else data-eventid="{{'dHe-2-'+index}}" data-comkey="{{$k}}"> 置顶 </view>
            <view class="_div data-v-24c259f8 removeM" bindtap="handleProxy" data-eventid="{{'Sq8-4-'+index}}" data-comkey="{{$k}}"> 删除 </view>
          </view>
          <view style="clear:both" class="_div data-v-24c259f8"></view>
        </view>
      </view>
    </view>
  </view>
</template>